{
  "name": "concurrent-tasks",
  "version": "3.0.0",
  "description": "A simple task runner which will run tasks concurrently while maintaining limits.",
  "author": "Samrith Shankar",
  "license": "MIT",
  "homepage": "https://concurrent-tasks.js.org",
  "repository": "https://github.com/samrith-s/concurrent-tasks.git",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "types": "lib/dts/index.d.ts",
  "files": [
    "lib",
    "LICENSE",
    "README"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "bootstrap": "yarn install && yarn --cwd docs install",
    "prepublishOnly": "yarn build:prod",
    "prebuild": "yarn clean",
    "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
    "build:prod": "NODE_ENV=production yarn build && size-limit",
    "build:dev": "NODE_ENV=development yarn build && size-limit",
    "clean": "rm -rf lib/",
    "test": "jest",
    "lint": "eslint {src,tests,docs}/**/* --cache",
    "prettify": "prettier --write {src,tests,docs}/**/*",
    "docs": "yarn --cwd docs"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "builtin-modules": "^3.3.0",
    "chalk": "^5.2.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "release-it": "^15.7.0",
    "rollup": "^4.6.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vitest": "^0.27.0"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "eslint --cache --fix"
  },
  "keywords": [
    "concurrent",
    "tasks",
    "processes",
    "threads",
    "tasks",
    "task",
    "worker",
    "process",
    "run",
    "concurrent tasks",
    "react concurrent task",
    "priority",
    "queue",
    "priority queue",
    "fifo",
    "scheduler",
    "schedule task"
  ]
}
